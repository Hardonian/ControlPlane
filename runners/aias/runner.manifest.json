{
  "name": "aias",
  "version": "0.1.0",
  "description": "AI Audit Service - compliance auditing, policy evaluation, and audit-trail generation",
  "entrypoint": {
    "command": "node",
    "args": [
      "scripts/adapters/runner-adapter.mjs",
      "--runner",
