# Service endpoints
TRUTHCORE_URL=http://localhost:3001
JOBFORGE_URL=http://localhost:3002
RUNNER_URL=http://localhost:3003

# Smoke test behavior
CP_SMOKE_ALLOW_MISSING=true

# Distribution mode (oss|cloud)
CONTROLPLANE_DISTRIBUTION=oss
CONTROLPLANE_DISTRIBUTION_CONFIG=

# Offline mode for sibling discovery
CONTROLPLANE_OFFLINE=false

# Demo mode (deterministic timestamp)
CONTROLPLANE_DEMO_TIME=2026-01-01T00:00:00.000Z

# Profiling
CONTROLPLANE_PROFILE=0
CONTROLPLANE_REQUEST_ID=

# Benchmarks
REDIS_HOST=localhost
REDIS_PORT=6379
BENCHMARK_RESULTS_DIR=./results
BENCHMARK_ITERATIONS=3
BENCHMARK_TIMEOUT=30000
HEALTH_CHECK_TIMEOUT=5000
BENCHMARK_LOG_LEVEL=info
LATENCY_TOLERANCE=0.15
THROUGHPUT_TOLERANCE=0.10

# Runner templates
REDIS_URL=redis://localhost:6379
RUNNER_ID=
PORT=3000
QUEUE_NAME=jobs
EXTERNAL_API_URL=https://api.example.com
EXTERNAL_API_KEY=
CONTROLPLANE_API_KEY=
