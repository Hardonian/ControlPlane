{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "contract:test": "vitest run --testNamePattern='Contract'",
    "health": "curl http://localhost:3000/health"
  },
  "dependencies": {
    "@controlplane/contracts": "^{{contractVersion}}",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@controlplane/contract-test-kit": "^{{contractVersion}}",
    "eslint": "^8.55.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "controlplane": {
    "contractVersion": "{{contractVersion}}",
    "contractCompatibility": {
      "min": "1.0.0",
      "max": "<2.0.0"
    },
    "status": "active"
  }
}
